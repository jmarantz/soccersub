{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "headers": [ {
      "source" : "**/*.@(js)",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=30"
      } ]
    }, {
      "source" : "**/*.@(css)",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=30"
      } ]
    } ]
  }
}
